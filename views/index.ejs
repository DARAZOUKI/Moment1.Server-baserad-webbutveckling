<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Course Management</title>
</head>
<body>
  <h1>Courses</h1>
  <ul>
    <% courses.forEach(course => { %>
      <li><%= course.coursename %> - <%= course.coursecode %> - <%= course.progression %> - <a href="#">Delete</a></li>
    <% }); %>
  </ul>
  <a href="/add">Add Course</a>
</body>
</html>
